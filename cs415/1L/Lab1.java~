import wheelsunh.users.*;
import java.awt.Color;



/** 
 * Lab1.java:  Displays a red circle.
 *
 * @author Brian Nippert
 * Date 9/2/14
 */
public class Lab1 
{
    //---------------- instance variables ------------------------------
     private Ellipse _circle;
     private Ellipse _circle2;
     private Rectangle _Rectangle;
     private Rectangle _Rectangle2;
     
    // -----------------------------------------------------------------
    /** Constructor for the Lab1 class.
      */
    public Lab1( )
    {
        _circle = new Ellipse( Color.BLUE );
        _circle.setLocation(50,60);
        
        _circle2 = new Ellipse( Color.RED );
        _circle2.setLocation(0,0);
        
        _Rectangle = new Rectangle( Color.ORANGE );
        _Rectangle.setLocation(200,150) ;
        
        _Rectangle2 = new Rectangle( Color.ORANGE );
        _Rectangle2.setSize(200,150) ;
    
    } 
    
    
    // -----------------------------------------------------------------
    /** main program creates a Frame and invokes the class constructor.
      * 
      * @param  args  the command line.
      */
    public static void main( String[] args )
    {
        Frame f = new Frame();
        Lab1 app = new Lab1();
    }
} //End of Class Lab1
